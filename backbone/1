//isNew is used to determine if the model is saved to server

var Vehicle = Backbone.Model .extend({
	
	defaults:{
	'color':'red',
	}
});
var Vehicle = Backbone.Model .extend({
validate :function(attrs){
var validColors = ['white','yellow'];	
var colorIsValid = function(attrs){
	if(!attrs.color)
	return true;
	return _(validColors).include(attrs.color);
}
if(!validColors(attrs)){'color must be one of these' +validColors.join("'") }
};
});

